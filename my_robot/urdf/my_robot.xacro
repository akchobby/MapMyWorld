<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
<xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo" />
  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="10.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".5 .3 .05"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".5 .3 .05"/>
      </geometry>
    </visual>

  </link>
  <link name="left_wheel_back">
	  
	<inertial>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <mass value="2"/>
	  <inertia 
		  ixx="0.1" ixy="0" ixz="0"
    		  iyy="0.1" iyz="0"
    		  izz="0.1"/>
	</inertial>
	
	<collision>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <geometry>
		  <cylinder radius="0.07" length="0.03"/>
	  </geometry>

	</collision>
	
	<visual>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <geometry>
		  <cylinder radius="0.07" length="0.03"/>
	  </geometry>

	</visual>
  </link>
  
  <link name="right_wheel_back">
	<inertial>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <mass value="2"/>
	  <inertia 
		  ixx="0.1" ixy="0" ixz="0"
    		  iyy="0.1" iyz="0"
    		  izz="0.1"/>
	</inertial>
	
	<collision>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <geometry>
		  <cylinder radius="0.07" length="0.03"/>
	  </geometry>

	</collision>
	
	<visual>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <geometry>
		  <cylinder radius="0.07" length="0.03"/>
	  </geometry>

	</visual>
  </link>
  
    <link name="left_wheel_front">
	  
	<inertial>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <mass value="2"/>
	  <inertia 
		  ixx="0.1" ixy="0" ixz="0"
    		  iyy="0.1" iyz="0"
    		  izz="0.1"/>
	</inertial>
	
	<collision>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <geometry>
		  <cylinder radius="0.07" length="0.03"/>
	  </geometry>

	</collision>
	
	<visual>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <geometry>
		  <cylinder radius="0.07" length="0.03"/>
	  </geometry>

	</visual>
  </link>
  
  <link name="right_wheel_front">
	<inertial>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <mass value="2"/>
	  <inertia 
		  ixx="0.1" ixy="0" ixz="0"
    		  iyy="0.1" iyz="0"
    		  izz="0.1"/>
	</inertial>
	
	<collision>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <geometry>
		  <cylinder radius="0.07" length="0.03"/>
	  </geometry>

	</collision>
	
	<visual>
	  <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
	  <geometry>
		  <cylinder radius="0.07" length="0.03"/>
	  </geometry>

	</visual>
  </link>
  <link name="camera">
  	<inertial>
  	  <origin xyz="0 0 0" rpy=" 0 0 0"/>
	  <mass value="0.1"/>
	  <inertia 
		  ixx="1e-6" ixy="0" ixz="0"
    		  iyy="1e-6" iyz="0"
    		  izz="1e-6"/>	
  	</inertial>
  	
  	<visual>
  	  <origin xyz="0 0 0" rpy=" 0 0 0"/>
  	  <geometry>
  	  	<box size="0.05 0.05 0.05"/>
  	  </geometry>
  	
  	</visual>
  	
  	<collision>
  	  <origin xyz="0 0 0" rpy=" 0 0 0"/>
  	  <geometry>
  	  	<box size="0.05 0.05 0.05"/>
  	  </geometry>
  	</collision>
  	
  	<box_inertia sizeX="0.05" sizeY="0.05" sizeZ="0.05" mass="0.1">
  	  <origin xyz="0 0 0" rpy=" 0 0 0"/>
  	</box_inertia>
  </link>

 <joint name="camera_optical_joint" type="fixed">
    <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707"/>
    <parent link="camera"/>
    <child link="camera_link_optical"/>
  </joint>

  <link name="camera_link_optical">
  </link>

	
  
  <link name="hokuyo">
        <inertial>
  	  <origin xyz="0 0 0" rpy=" 0 0 0"/>
	  <mass value="1e-5"/>
	  <inertia 
		  ixx="1e-6" ixy="0" ixz="0"
    		  iyy="1e-6" iyz="0"
    		  izz="1e-6"/>	
  	</inertial>
  	<visual>
  	  <origin xyz="0 0 0" rpy=" 0 0 0"/>
  	  <geometry>
  	  	<mesh filename="package://my_robot/meshes/hokuyo.dae"/>
  	  </geometry>
  	</visual>
  	
  	<collision>
  	  <origin xyz="0 0 0" rpy=" 0 0 0"/>
  	  <geometry>
  	  	<box size="0.1 0.1 0.1"/>
  	  </geometry>
  	</collision>
  </link>
  <link name="lidar_support">
  	<inertial>
  	  <origin xyz="0 0 0" rpy=" 0 0 0"/>
	  <mass value="0.1"/>
	  <inertia 
		  ixx="1e-6" ixy="0" ixz="0"
    		  iyy="1e-6" iyz="0"
    		  izz="1e-6"/>	
  	</inertial>
  	
  	<visual>
  	  <origin xyz="0 0 0" rpy=" 0 0 0"/>
  	  <geometry>
  	  	<box size="0.03 0.03 0.10"/>
  	  </geometry>
  	
  	</visual>
  	
  	<collision>
  	  <origin xyz="0 0 0" rpy=" 0 0 0"/>
  	  <geometry>
  	  	<box size="0.03 0.03 0.10"/>
  	  </geometry>
  	</collision>
  
  </link>
  
  <joint type="continuous" name="left_wheel_back_hinge">
    <origin xyz="-0.25 0.2 0.0" rpy="0 0 0"/>
    <child link="left_wheel_back"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>
  
  <joint type="continuous" name="right_wheel_back_hinge">
    <origin xyz="-0.25 -0.2 0.0" rpy="0 0 0"/>
    <child link="right_wheel_back"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>
  
  <joint type="continuous" name="left_wheel_front_hinge">
    <origin xyz="0.25 0.2 0.0" rpy="0 0 0"/>
    <child link="left_wheel_front"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>
  
  <joint type="continuous" name="right_wheel_front_hinge">
    <origin xyz="0.25 -0.2 0.0" rpy="0 0 0"/>
    <child link="right_wheel_front"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>
  
  <joint type="fixed" name="camera_joint">
    <origin xyz="0.3 0 0" rpy="0 0 0"/>
    <child link="camera"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>
  
  <joint type="fixed" name="support_joint">
    <origin xyz="0.15 0 0.05" rpy="0 0 0"/>
    <child link="lidar_support"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>
  
  <joint type="fixed" name="hokuyo_joint">
    <origin xyz="0 0 0.10" rpy="0 0 0"/>
    <child link="hokuyo"/>
    <parent link="lidar_support"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>
 <gazebo reference="left_wheel_back">
    <material>Gazebo/Black</material>
 </gazebo>
 
 <gazebo reference="right_wheel_back">
    <material>Gazebo/Black</material>
 </gazebo>
 
  <gazebo reference="left_wheel_front">
    <material>Gazebo/Black</material>
 </gazebo>
 
 <gazebo reference="right_wheel_front">
    <material>Gazebo/Black</material>
 </gazebo>
 
 <gazebo reference="chassis">
    <material>Gazebo/Red</material>
 </gazebo>
 
 <gazebo reference="camera">
    <material>Gazebo/Green</material>
 </gazebo>
</robot>
